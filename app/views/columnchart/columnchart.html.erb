<html>
  <head>
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript">
      google.load("visualization", "1", {packages:["corechart"]});
google.setOnLoadCallback(drawChart);
function drawChart() {
  // Se crea la tabla donde se cargaran barrios y precios
  var data = new google.visualization.DataTable();
  data.addColumn('string', 'Barrio');
  data.addColumn('number', 'Precio promedio por metro cuadrado');
  
  // Se obtiene la lista de barrios desde Android
  var barrios = Android.getBarriosGraficoBarras();
  var cantidadBarrios = barrios.length;
  
  // Se completan los valores para cada barrio y se llena la tabla
  for (var i = 0; i < cantidadBarrios; i++) {
    data.addRows([barrios[i], Android.getPrecioPromedioDeBarrio(barrios[i]);
  }


  var options = {
    title: 'Prueba de grÃ¡fico de barras',
    hAxis: {title: 'Barrios', titleTextStyle: {color: 'red'}}
  };

  var chart = new google.visualization.ColumnChart(document.getElementById('chart_div'));

  chart.draw(data, options);
}
    </script>
  </head>
  <body>
    <div id="chart_div" style="width: 900px; height: 500px;"></div>
  </body>
</html>